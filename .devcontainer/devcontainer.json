{
  "extensions": [
    "github.vscode-codeql",
    "vsls-contrib.codetour"
  ],
   "customizations": {
    "codespaces": {
      "openFiles": ["tutorial.code-workspace", "README.md"]
    },
    "vscode": {
      // open workspace in a new window
      "workbench.startupEditor": "tutorial.code-workspace",
    }
  },
  "onCreateCommand": "bash .devcontainer/onCreateCommand.sh",
  "postCreateCommand": "bash .devcontainer/postCreateCommand.sh"
}
