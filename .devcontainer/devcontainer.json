{
  "extensions": [
    "github.vscode-codeql",
    "vsls-contrib.codetour"
  ],
   "customizations": {
    "codespaces": {
      "openFiles": ["README.md"]
    },
    "vscode": {
      "vscode.openFolder": "vscode.window.activeTextEditor.document.uri.fsPath('tutorial.code-workspace')"
    }
  },
  "onCreateCommand": "bash .devcontainer/onCreateCommand.sh",
  "postCreateCommand": "bash .devcontainer/postCreateCommand.sh"
}
